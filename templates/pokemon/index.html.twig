{% extends 'base.html.twig' %}

{% block title %}Hello {{ controller_name }}!{% endblock %}

{% block body %}

    <div>
        {% for pokemon in pokemons %}
            <p>{{ pokemon.name }}</p>
            <img width="120px" src="{{ pokemon.image }}">
        {% endfor %}
        <p> {{ trainer.getName() }}</p>
        <p> {{ trainer.getTeam() }}</p>
    </div>
{% endblock %}
